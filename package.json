{
  "name": "solpilot",
  "description": "SolPilot is a command-line interface (CLI) tool designed to interact with the Solana blockchain. It allows users to perform essential wallet operations such as generating keypairs, requesting airdrops of SOL tokens, and sending SOL tokens to other wallets. The tool is built using the Oclif framework and leverages Solana's Web3.js library for blockchain interactions.",
  "version": "1.3.1",
  "author": "0xjesus",
  "bin": {
    "solpilot": "./bin/run.js"
  },
  "bugs": "https://github.com/0xjesus/solpilot/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@solana/web3.js": "^1.95.1",
    "chalk": "^5.3.0",
    "inquirer": "^10.1.2",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/prompts": "^2.4.9",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "/theme.json"
  ],
  "homepage": "https://github.com/0xjesus/solpilot",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "solpilot",
    "dirname": "solpilot",
    "theme": "theme.json",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "0xjesus/solpilot",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": ""
  },
  "types": "dist/index.d.ts"
}
